<?xml version="1.0" encoding="UTF-8"?>
<project>
  <name>cdda</name>
  <version>1.0.0</version>
  <makefile>..\Makefile</makefile>
  <neocorePath>..\..\</neocorePath>
  <buildPath>..\..\build</buildPath>
  <distPath>..\..\dist</distPath>
  <gfx>
    <DAT>
      <chardata>
        <setup>
          <starting_tile>256</starting_tile>
        </setup>
        <pict id="k7_sprite">
          <file>assets\gfx\k7.png</file>
        </pict>
        <scrl id="spectrum02_sprite">
          <file>assets\gfx\02-spectrum.png</file>
        </scrl>
      </chardata>
    </DAT>
  </gfx>
  <emulator>
    <raine>
      <exeFile>..\..\build\raine\raine32.exe</exeFile>
    </raine>
    <mame>
      <exeFile>..\..\build\mame\mame64.exe</exeFile>
    </mame>
  </emulator>
  <compiler>
    <name>gcc</name>
    <version>2.95.2</version>
    <path>..\..\build\gcc\gcc-2.95.2</path>
    <includePath>..\..\build\include</includePath>
    <libraryPath>..\..\build\lib</libraryPath>
    <systemFile>..\..\build\system\neocd.x</systemFile>
  </compiler>
  <sound>
    <cdda>
      <dist>
        <iso>
          <format>mp3</format>
        </iso>
      </dist>
      <tracks>
        <track><!-- track id 1 is reserved for the binary program -->
          <id>2</id>
          <file>assets\sounds\cdda\The Moon - High Orbit.wav</file>
          <pregap>00:02:00</pregap>
        </track>
        <track>
          <id>3</id>
          <file>assets\sounds\cdda\Sweet_Mermaids.wav</file>
          <pregap>00:02:00</pregap>
        </track>
        <track>
          <id>4</id>
          <file>assets\sounds\cdda\Level One.wav</file>
          <pregap>00:02:00</pregap>
        </track>
        <track>
          <id>5</id>
          <file>assets\sounds\cdda\pong.mp3</file>
          <pregap>00:02:00</pregap>
        </track>
      </tracks>
    </cdda>
  </sound>
</project>